{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "jquery-wp-demo-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/jqueryWpDemo/JqueryWpDemoWebPart.js",
          "manifest": "./src/webparts/jqueryWpDemo/JqueryWpDemoWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {
    "jquery" : "./node_modules/jquery/dist/jquery.min.js",
    "jqueryui" :"./node_modules/jqueryui/jquery-ui.min.js"
    },
  "localizedResources": {
    "JqueryWpDemoWebPartStrings": "lib/webparts/jqueryWpDemo/loc/{locale}.js"
  }
}
